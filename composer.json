{
    "name": "tikaraj21/tika_widget",
    "description": "Developed Using Yii2 Framework",
    "keywords": ["yii2", "framework", "project"],
    "homepage": "",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/tikaraj21/tika_widget/issues?state=open",               
        "source": "https://github.com/tikaraj21/tika_widget"
    },
    "minimum-stability": "dev",
    "require": {
         "php": ">=5.4.0",
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-ignore-files": {
            "bower-asset/adminlte": [
                "bootstrap",
                "documentation",
                "build",
                "dist/img",
                "pages",
                "plugins",
                "*.*",
                "changelog",
                "LICENSE"
            ]
        },
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
